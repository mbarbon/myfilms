	url_listPages[7] = [];
	$('#menuv2 ul').css("display", "none");												
	var labelPages = new Array();
	labelPages = ["insala", "prossimamente", "homevideo", "m_multimedia", "notizie", "classifiche", "festivalpremi", "community"];
	var listPages = new Array();
	
	listPages[0] = ["0.3","0.4","0.5","0.6","0.30","0.7","0.8","0.9"];
	listPages[1] = [];
	listPages[2] = ["0.13,0.13.2", "0.13.2.1", "0.13.1", "0.13.1.1"];
	listPages[3] = ["0.14.1","0.14.2","0.14.3","0.14.4", "0.14.5","0.14.6","0.14.7","0.14.8"];
	listPages[4] = ["0.15,0.15.1, 0.16", "http://newscontrol.repubblica.it/tag/cinema", "http://starcontrol.repubblica.it/?kat=Cinema", "http://www.repubblica.it/spettacoli_e_cultura/index.html", "http://www.repubblica.it/persone/index.html"];
	listPages[5] = ["0.11.4,0.11", "0.11.7", "0.11.3", "0.11.1", "0.11.2", "0.11.5", "0.11.6"];
	listPages[6] = ["0.17.1,0.17,0.17.1.1", "0.17.2,0.17.2.1", "0.17.3,0.17.3.1", "0.17.4,0.17.4.1"];
	listPages[7] = [];
	var strtest = new String();
	var checkedMenu = new Boolean();
	checkedMenu = false;
	for(var a = 0; a < listPages.length; a++) {
		for(var b = 0; b < listPages[a].length; b ++) {
			if(listPages[a][b].toString().indexOf(",") != -1) {
				splittedPage = new Array();
				splittedPage = listPages[a][b].split(",");
				for(var c = 0; c < splittedPage.length; c ++) {																
					if(pageName_js == splittedPage[c]) {
						// Se sono gli speciali.
						if(a == 6) {
							if(pageName_js.split(".").length > 3) {
								var tempArr = pageName_js.split('.');
								var tempStr = tempArr[0]+tempArr[1]+tempArr[2];
								//alert(tempStr);
								$("#"+tempStr).addClass("activeMenuSubvoice");	
							} else {																				
								$("a[href='"+url_listPages[a][b]+"']").addClass("activeMenuSubvoice");
								//alert(listPages[a][b]);
							}	
								if(a == 0) {
									$('#'+labelPages[a]).addClass("currentmenuhome");
									$('#t_'+labelPages[a]).css("display", "block");
								} else {
									$('#'+labelPages[a]).addClass("currentmenu");
									$('#t_'+labelPages[a]).css("display", "block");
								}
							
							checkedMenu = true;
							break;
						} else {
							$("a[href='"+url_listPages[a][b]+"']").addClass("activeMenuSubvoice");
							//alert(listPages[a][b]);
							if(a == 0) {
								$('#'+labelPages[a]).addClass("currentmenuhome");
								$('#t_'+labelPages[a]).css("display", "block");
							} else {
								$('#'+labelPages[a]).addClass("currentmenu");
								$('#t_'+labelPages[a]).css("display", "block");
							}
							checkedMenu = true;
							break;
							}
						
					}	
				}
			} else {						
				if(pageName_js == listPages[a][b]) {
					$("a[href='"+url_listPages[a][b]+"']").addClass("activeMenuSubvoice");
					//alert(listPages[a][b]);
					if(a == 0) {
						$('#'+labelPages[a]).addClass("currentmenuhome");
						$('#t_'+labelPages[a]).css("display", "block");
					} else {
						$('#'+labelPages[a]).addClass("currentmenu");
						$('#t_'+labelPages[a]).css("display", "block");
					}
					checkedMenu = true;
					break;
				}	
			}
		}
	}
	if(checkedMenu == true) {					
	} else {					
		if(pageName_js == "0.10") {
			$('#menuv2 ul').css("display", "none");
			$('#prossimamente').addClass("currentmenu");
			$('#t_prossimamente').css("display", "block");
		} else if(pageName_js == "0.19") {
			$('#menuv2 ul').css("display", "none");
			$('#community').addClass("currentmenu");
			$('#t_community').css("display", "block");
		} else {
			$('#insala').addClass("currentmenuhome");
			$('#t_insala').css("display", "block");
		}
	}
	

	function showMenuTab(id) {
		if(id == "insala") {
			$('.currentmenu').removeClass("currentmenu");				
			$('#'+id).addClass("currentmenuhome");
			$('#menuv2 ul').css("display", "none");
			$('#t_'+id).css("display", "block");
		} else {
			$('.currentmenu').removeClass("currentmenu");
			$('.currentmenuhome').removeClass("currentmenuhome");
			$('#'+id).addClass("currentmenu");
			$('#menuv2 ul').css("display", "none");
			$('#t_'+id).css("display", "block");
		}			
		
		
	}
</script>
