				<script>
					// Gestore evento onLoad della pagina
					$(document).ready(function(){
						// Test Posizioni pubblicitarie a seconda della pagina in cui ci si trova
						// HOMEPAGE
						if(cPage == "home") {
							//alert("home");
							var adDivList = ['oas-Middle1'];//'oas-Right','oas-Left','oas-Left1','oas-Left2','oas-Right1','oas-Right2','oas-Middle1'
							OAS_VIS_MAIN(adDivList, true, true);												
						// ALTRE PAGINE
						} else if(cPage == "other") {
							//alert("other");
							var adDivList = ['oas-Middle1'];//'oas-Right','oas-Middle1'
							OAS_VIS_MAIN(adDivList, false, true);												
						}
						// Condizione per verifica dimensioni immagini fotogallery
						if($("img.fotogal").length > 0) {
							checkImageDims();
						}
					 });					
					function checkImageDims() {
						//alert($("img.fotogal").height()+" - "+$("img.fotogal").width()+" -- LIMITS: h 600 W 670");
						if($("img.fotogal").height() > 600) {	
							rapporto = $("img.fotogal").width()/$("img.fotogal").height();		
							$("img.fotogal").css("height", "600px");		
							$("img.fotogal").css("width", (600*rapporto)+"px");
							$("img.fotogal").css("visibility", "visible");		
							//alert(rapporto);
							return;
						} else if($("img.fotogal").width() > 600) {
							rapporto = $("img.fotogal").height()/$("img.fotogal").width();		
							$("img.fotogal").css("height", (600*rapporto)+"px");		
							$("img.fotogal").css("width", "600px");
							$("img.fotogal").css("visibility", "visible");
							//alert(rapporto);
							return;
						} else {
							$("img.fotogal").css("visibility", "visible");
						}
					}
					//$("img.fotogal").bind("load", function(e) {checkImageDims();})					
				</script>
